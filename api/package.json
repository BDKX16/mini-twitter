  "author": "Twitter Clone Team",
  "license": "MIT",
  "dependencies": {
    "@types/multer": "^2.0.0",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.0",
    "mongoose-unique-validator": "^4.0.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.6.12"
