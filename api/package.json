{
  "name": "twitter-clone-api",
  "version": "1.1-stable",
  "description": "API backend para clon de Twitter con MongoDB y Redis",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint .",
    "seed": "ts-node seed-data.js",
